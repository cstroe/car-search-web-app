<html>
<body>
<h1>Vehicle Details</h1>

<table>
<tr>
    <th>id</th>
    <th><!-- interested --></th>
    <th>dealer</th>
    <th>year</th>
    <th>make</th>
    <th>model</th>
    <th>trim</th>
    <th>miles</th>
    <th>price</th>
    <th>vin</th>
    <th>url</th>
    <th>action</th>
</tr>
<tr>
  <td><a href="/vehicle/${vehicle.id()}">${vehicle.id()}</a></td>
  <td>
    <#if vehicle.interested()>
      <a href="/mark_not_interested/${vehicle.id()}">⭐</a>
    <#else>
      <a href="/mark_interested/${vehicle.id()}">➕</a>
    </#if>
  </td>
  <td>${vehicle.dealer()}</td>
  <td>${vehicle.year()}</td>
  <td>${vehicle.make()}</td>
  <td>${vehicle.model()}</td>
  <td>${vehicle.trim()}</td>
  <td>${vehicle.miles()}</td>
  <td>${vehicle.price()}</td>
  <td>${vehicle.vin()}</td>
  <td><a href="${vehicle.url()}" target="_blank">LINK</a></td>
  <td><a href="/set_as_unavailable/${vehicle.id()}">SET UNAVAILABLE</a></td>
</tr>
</table>

</body>
</html>
